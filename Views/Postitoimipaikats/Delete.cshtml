@model TilausJärjestelmä.Models.Postitoimipaikat

@{
    ViewBag.Title = "Delete";
}



<h3>Haluatko varmasti poistaa tämän postitoimipaikan?</h3>
<div>
    <h2>Postitoimipaikka</h2>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Postitoimipaikka)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Postitoimipaikka)
